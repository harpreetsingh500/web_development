<div id="user_page">
  <h2 class='titles'>Personal User Page</h2>
  <p><%= "Name: #{@name.capitalize}" %></p>
  <p><%= "Email: #{@email}" %></p>
  <p><%= "Interests: #{@interests}" %></p>
  <p></p>
  <p></p>
</div>

<div id="other_users">
  <h2 class='titles'>Other Users</h2>
  <% @other_users.each do |name| %>
    <p>Name: <a href=<%= "/user/#{name}" %>><%= "#{name.capitalize}" %></a></p>
  <% end %>
</div>  